{
    "manifest_version": 3,
    "name": "Quote of the Day",
    "version": "1.0",
    "description": "Displays a new inspirational or humorous quote every time you open a new tab.",
    "permissions": ["storage"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "chrome_url_overrides": {
      "newtab": "popup.html"
    }
  }
  